interface s0/0/1
 ip nat outside 
interface g0/0
 ip nat inside
interface g0/1
 ip nat inside  
exit 

"NUMBERED ACL"
access-list 1 permit 192.168.1.0 0.0.0.255
#access-list 1 deny 192.168.1.0 0.0.0.255

"NAMED ACL"
ip access-list standard STUDENT
 permit 192.168.1.0 0.0.0.255
 #deny 192.168.1.0 0.0.0.255
 permit any
 exit

"STATIC NAT"
ip nat inside source static 192.168.1.23 200.51.100.10

"PAT"
ip nat inside source list 1 pool LANA overload 
ip nat inside source list STUDENT interface s0/0/1 overload 

